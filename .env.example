# Environment Configuration
NODE_ENV=production

# Server Configuration
PORT=5000

# Database Configuration (if using PostgreSQL)
# DATABASE_URL=postgresql://username:password@host:port/database

# Session Secret
SESSION_SECRET=your-secure-session-secret-here

# OAuth Configuration (loaning.ai)
OAUTH_CLIENT_ID=your-client-id
OAUTH_CLIENT_SECRET=your-client-secret
OAUTH_CALLBACK_URL=https://preplounge.ai/api/auth/callback

# Domain Configuration
FRONTEND_URL=https://preplounge.ai

# Vite Frontend Configuration
# Backend URL for API proxy (defaults to http://localhost:5000 in development)
# For production deployments, set this to your backend service URL
# Example: VITE_BACKEND_URL=https://api.dev.preplounge.ai
VITE_BACKEND_URL=http://localhost:5000
